/* ---------------- template styles ------------------------- */

table#main-table td#header {
	background-image: url( ../images/gov-header-region-bg.gif);
}

table#main-table td#menu-panel {
	border-right: solid 1px #9D9FA1;
	background-image: url(../images/gov-menu-panel-bg.gif);
	background-position: right top;
	background-repeat: repeat-y;
}
	

/* ---------------- header styles ------------------ */
div#header-div {
    background-image: url( ../images/gov-header-bg.gif);
}

div#header-div div.left-logo {
	background-image:url("../images/gov-header-logo.gif");
	margin-left:56px;
	margin-top:34px;
	width:220px;
}
div#header-div div.middle-ad {
	display: none;
}

div#header-div div.right-logo {
	/*background-image: url( ../images/wsas-mgt-logo.gif);*/
	color: #794400;
} 

div#header-div div.header-links {
	margin-top:2px;
}
div#header-div div.header-links div.right-links {
	margin-right: 0px;
	height: 35px;
	padding-top: 0px;
}
div#header-div div.header-links div.right-links ul {
	background-image:url("../images/gov-header-links-bg.gif");
	background-position:left top;
	background-repeat:repeat-x;
	padding-left: 25px;
	padding-right: 15px;
	padding-top: 9px;
	padding-bottom: 7px;
}

/* ------------- menu styles ---------------------- */
div#menu {
}

div#menu ul.main {
}

div#menu ul.main li {
}

div#menu ul.main li.normal {
}

div#menu ul.main li a.menu-home {
	color: #794400;
    background-image: none;
    height: 30px;
    line-height: 30px;
}

div#menu ul.main li a.menu-home:hover {
	color: #f47b20;
	background-image: none;
	background-color: #F3E9DF;
}

div#menu ul.main li.menu-header {
	background-image: url(../images/gov-menu-header.gif);
	background-position: left bottom;
	background-repeat: repeat-x;
	height: 32px;
	line-height: 32px;
}

div#menu ul.main li.menu-header img {
	margin-right:-14px;
	margin-top:7px;
}
div#menu ul.main li a.menu-default {
}

div#menu ul.main li a.menu-default:hover {
	background-color: #F3E9DF;
	border-bottom: solid 1px #D5B799;
	border-top: solid 1px #D5B799;
	color: #00447C;
}

div#menu ul.main li.menu-disabled-link {
	background-color: #E8E8E8;
}

